	<build>
		<finalName>foodmanagement</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-war-plugin</artifactId>
					<version>3.2.2</version>
					<!--<version>3.4.1-SNAPSHOT</version> -->
					<configuration>
						<outputDirectory>D:\ezee cloud solution\apache-tomcat-8.5.95-windows-x64\apache-tomcat-8.5.95\webapps</outputDirectory>
					</configuration>
				</plugin>
				<plugin>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.0</version>
					<!--<version>3.11.0</version> -->
					<configuration>
						<source>1.8</source>
						<target>1.8</target>
						<fork>true</fork>
						<!-- <executable>C:\Program Files\Java\jdk1.8.0_202\bin</executable> -->
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
